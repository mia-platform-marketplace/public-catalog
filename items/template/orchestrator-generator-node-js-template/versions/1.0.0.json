{
  "$schema": "../../manifest.schema.json",
  "categoryId": "nodejs",
  "description": "A starting template to build your Orchestrator Generator and connect it to Mia-Platform",
  "documentation": {
    "type": "externalLink",
    "url": "https://docs.mia-platform.eu/docs/console/company-configuration/providers/extensions/orchestrator-generator/overview"
  },
  "image": {
