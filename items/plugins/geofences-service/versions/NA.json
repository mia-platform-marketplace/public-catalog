{
  "$schema": "../../manifest.schema.json",
  "categoryId": "addgeo",
  "description": "The Geofences Service Plugin handles geofences and related resources and actions.",
  "image": {
    "localPath": "../assets/geofences-service.png"
  },
  "itemId": "geofences-service",
  "name": "Geofences Service",
  "releaseStage": "deprecated",
  "resources": {
    "services": {
      "geofences-service": {
        "type": "plugin",
        "name": "geofences-service",
        "description": "The Geofences Service Plugin handles geofences and related resources and actions.",
        "dockerImage": "nexus.mia-platform.eu/plugins/geofences-service:1.0.0",
        "containerPorts": [
          {
            "name": "http",
            "from": 80,
            "to": 8080
          }
        ],
        "defaultEnvironmentVariables": [
          {
            "name": "PORT",
            "value": "8080",
            "valueType": "plain"
          },
          {
            "name": "URL",
            "value": "http://crud-service/CHANGE_WITH_YOUR_CRUD_NAME",
            "valueType": "plain"
          },
          {
            "name": "SECRET",
            "value": "CHANGE_WITH_YOUR_SECRET",
            "valueType": "plain"
          },
          {
            "name": "RADIUS",
            "value": "300",
            "valueType": "plain"
          }
        ]
      }
    }
  },
  "supportedBy": "Mia-Platform",
  "supportedByImage": {
    "localPath": "../../../../assets/img/mia-platform-logo-2023.png"
  },
  "tenantId": "mia-platform",
  "type": "plugin",
  "visibility": {
    "public": true
  },
  "releaseDate": "2020-08-05T08:24:02.500Z"
}
