            {
              "from": 50051,
              "name": "grpc",
              "to": 50051
            }
          ],
          "defaultArgs": [
            "-c",
            "/etc/envoy/envoy.json",
            "-l",
            "{{LOG_LEVEL}}",
            "--log-format",
            "{\\"level\\":\\"%l\\",\\"time\\":\\"%Y-%m-%dT%T.%fZ\\",\\"scope\\":\\"%n\\",\\"message\\":\\"%j\\"}",
            "--drain-time-s",
            "50"
          ],
          "defaultConfigMaps": [
            {
              "files": [],
              "link": {
                "targetSection": "endpoints"
              },
              "mountPath": "/etc/envoy",
